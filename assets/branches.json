---

---

{
  "branches": [
    {%- assign included = 'index.md,index.html' | split: ',' -%}
    {%- assign excluded = '404.md' | split: ',' -%}

    {%- assign indices = site.pages | where_exp: 'page', 'included contains page.name' -%}
    {%- assign wikis = indices | where: 'layout', 'wiki' -%}
    {%- for wiki in wikis -%}

      {%- assign wiki_prop = wiki.wiki_cat | default: 'none' -%}

      {%- assign desc = wiki.description | escape -%}
      {%- assign title = wiki.title | escape | default: 'Untitled' -%}
      {%- assign canonical_url = site.url | append: wiki.url -%}

      {
        "title": "{{ title }}",
        "title-aria": "{{ title }}",
        "description": "{{ desc }}",
        "description-aria": "{{ desc | default: 'No description provided' }}",
        "path": "{{ wiki.url }}",
        "canonical-url": "{{ canonical_url }}",
        "category": "{{ wiki_prop }}"
      }{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ]
}
