{
    "$schema":"https://json-schema.org/draft/2020-12/schema",
    "$id":"https://taushsampley.com/schemas/datetime",
    "description": "An ISO 8601 formatted datetime string",
    "type":"string",
    "format":"datetime",
    "pattern": "^[0-9]{1,4}-[0-9]{1,2}-[0-9]{1,2}[T\\s][0-9]{2}:[0-9]{2}:[0-9]{2}(\\.[0-9]+)$"
}
