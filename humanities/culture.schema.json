---
---
{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "{{ page.url | relative_url }}",
  "$comment": "Etymology - https://www.etymonline.com/word/culture",
  "type": "object",
  "properties": {
    "name": { "type": "string" },
    "food": { "type": "array", "items": { "$ref": "{{ humanities/food/food | relative_url }}"} },
    "music": { "type": "array", "items": { "anyOf" [
      { "$ref": "{{ humanities/music/song | relative_url }}" },
      { "$ref": "{{ humanities/music/album | relative_url }}" },
      { "$ref": "{{ humanities/music/artist | relative_url }}" }
    ]}},

  }
}
